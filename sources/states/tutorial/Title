#pragma once

struct TitleTutorialStep : public TutorialStep {
    void enter(SessionComponent* , TutorialEntities* ) {
    }
    bool mustUpdateGame(SessionComponent* , TutorialEntities* ) {
        return false;
    }
    bool canExit(SessionComponent* , TutorialEntities* ) {
        return true;
    }
    void exit(SessionComponent* , TutorialEntities* ) {
    }
};
